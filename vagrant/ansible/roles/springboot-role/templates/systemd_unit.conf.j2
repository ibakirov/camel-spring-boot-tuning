[Unit]
Description={{ spring_boot_application_id }}
After=syslog.target

[Service]
User={{ spring_boot_username }}
Environment="JAVA_OPTS={{ spring_boot_java_opts }}"
ExecStart=/usr/bin/java $JAVA_OPTS -jar {{ spring_boot_file_fq }} {{ spring_boot_start_opts }} 
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target